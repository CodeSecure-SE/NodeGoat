			"extensions": [
				"dbaeumer.vscode-eslint",
				"github.vscode-github-actions",
				"MS-SarifVSCode.sarif-viewer",
				"CodeSecure.vscode-codesonar"
			],
			"settings": {}
		}
	}
}
